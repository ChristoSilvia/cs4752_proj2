<launch>
	<node name="bezier" pkg="cs4752_proj2" type="bezier.py">
	</node>
	<node name="controller" pkg="cs4752_proj2" type="controller.py" output="screen">
	</node>
	
	<node name="joint_action_server" pkg="cs4752_proj2" type="joint_action_server.py" output="screen">
	</node>
	<node name="test" pkg="cs4752_proj2" type="test-bezier.py" output="screen">
	</node>
	<node name="SimpleHTTPServer" pkg="cs4752_proj2" type="SimpleHTTPServer.sh">
	</node>
	<include file="$(find rosbridge_server)/launch/rosbridge_websocket.launch" >
		<arg name="port" value="9090"/>
	</include>

</launch>
